<body>
	<form method="POST" action="/login">
  <div  class="form-group">
    <label for="textInput">User Id</label>
    <input type="text" class="form-control" id="textInput" name = "uid" placeholder="123123" required>
  </div>
  <div  class="form-group">
    <label for="passInput">Password</label>
    <input type="password" class="form-control" id="textInput" name = "pass" placeholder="password" required>
  </div>
  <a href="/">
   <button type="submit" class="btn btn-primary">Submit</button></a>
  </form>


